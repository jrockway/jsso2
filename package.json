{
    "name": "jsso2",
    "version": "0.0.0-pre",
    "license": "Apache-2.0",
    "repository": "https://github.com/jrockway/jsso2",
    "scripts": {
        "all": "env-path -p env.test concurrently -c blue.dim,magenta.dim,green.dim -k npm:backend npm:envoy npm:dev",
        "generate:go": "go generate ./...",
        "generate:ts": "protoc -Iprotos --js_out=import_style=commonjs:src/protos --grpc-web_out=import_style=typescript,mode=grpcwebtext:src/protos protos/*.proto",
        "backend": "go run ./cmd/jsso2 2>&1 | jlog -C",
        "envoy": "envoy -c envoy.yaml -l critical",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "start": "sirv public",
        "test:go": "env-path -p env.test go test ./... -race",
        "cover:go": "go test ./... -coverprofile=cover.out -covermode=atomic && go tool cover -html cover.out -o coverage.html",
        "test": "concurrently npm:test:go",
        "lint:svelte": "svelte-check",
        "lint:go": "golangci-lint run",
        "lint": "concurrently npm:lint:svelte npm:lint:go"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "@rollup/plugin-typescript": "^4.0.0",
        "@tsconfig/svelte": "^1.0.0",
        "concurrently": "^5.3.0",
        "env-path": "^1.0.7",
        "rollup": "^2.3.4",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^6.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "sirv-cli": "^1.0.6",
        "svelte": "^3.0.0",
        "svelte-check": "^1.0.0",
        "svelte-preprocess": "^4.0.0",
        "svelte-spa-router": "^2.2.0",
        "tslib": "^2.0.0",
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "google-protobuf": "^3.13.0",
        "grpc-web": "^1.2.1"
    }
}
