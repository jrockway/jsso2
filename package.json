{
    "name": "jsso2",
    "version": "0.0.0-pre",
    "scripts": {
        "all": "concurrently -c blue.dim,magenta.dim,green.dim -k npm:backend npm:envoy npm:dev",
        "generate": "go generate ./...",
        "backend": "go run ./cmd/jsso2 --debug_address=0.0.0.0:8081 2>&1 | jlog -C",
        "envoy": "envoy -c envoy.yaml -l critical",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "start": "sirv public --host",
        "validate": "svelte-check"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "@rollup/plugin-typescript": "^4.0.0",
        "@tsconfig/svelte": "^1.0.0",
        "rollup": "^2.3.4",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^6.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "sirv-cli": "^1.0.6",
        "svelte": "^3.0.0",
        "svelte-check": "^1.0.0",
        "svelte-preprocess": "^4.0.0",
        "tslib": "^2.0.0",
        "typescript": "^3.9.3"
    },
    "dependencies": {
        "concurrently": "^5.3.0"
    }
}
